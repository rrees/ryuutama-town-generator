{
	"name": "ryuutama-town-generator",
	"version": "0.0.2",
	"license": "MIT",
	"main": "./lib/main.js",
	"directories": [
		"src",
		"lib"
	],
	"keywords": [
		"procedural generation"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/rrees/ryuutama-town-generator"
	},
	"engines": {
		"node": ">=7"
	},
  "devDependencies": {
    "jest": "^20.0.1",
    "typescript": "^2.3.2"
  },
  "scripts": {
    "test": "jest",
    "build": "tsc"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/*.(ts|tsx|js)"
    ]
  }
}
